{
    "name": "php-etl/fast-map",
    "description": "This library implements a compiled data mapping library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiboko SAS",
            "homepage": "http://kiboko.fr"
        },
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "nikic/php-parser": "^4.9@dev",
        "symfony/property-access": "^5.2",
        "symfony/expression-language": "^5.2",
        "symfony/cache": "^5.4",
        "doctrine/inflector": "^2.0",
        "php-etl/metadata": "^0.2@dev",
        "php-etl/contracts": "^0.1.x-dev",
        "adlawson/vfs": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\FastMap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Kiboko\\Component\\FastMap\\": "spec/",
            "unit\\Kiboko\\Component\\FastMap\\": "unit/",
            "functional\\Kiboko\\Component\\FastMap\\": "functional/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "friends-of-phpspec/phpspec-code-coverage": "^6.0",
        "phpspec/phpspec": "^7.0",
        "johnkary/phpunit-speedtrap": "*",
        "mybuilder/phpunit-accelerator": "*",
        "phpunit/php-invoker": "*"
    }
}
